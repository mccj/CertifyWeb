{% if classNamePrefixXnRestfulResult != 'XnRestfulResult_' and classNamePrefixXnPaged != 'XnPaged_' -%}
export const {{ ClassName }}Columns = {
{% for property in Properties -%}
    {{ property.PropertyName | capitalize }}: { dataIndex: '{{ property.PropertyName }}', title: '{% if property.HasDescription -%}{{ property.Description | escape_once | newline_to_br }}{% else -%}{{ property.PropertyName }}{% endif %}' },
{% endfor -%}
};

